@model IEnumerable<endep.Models.Persona.Persona>
@{
    ViewBag.Title = "Inicio";
}

<!-- page content -->
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Usuarios inscritos </h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li>
                                        <a href="#">Settings 1</a>
                                    </li>
                                    <li>
                                        <a href="#">Settings 2</a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="table-responsive">

                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>@Html.DisplayNameFor(model => model.UserId)</th>
                                        <th>@Html.DisplayNameFor(model => model.Email)</th>
                                        <th>@Html.DisplayNameFor(model => model.Name)</th>
                                        <th>@Html.DisplayNameFor(model => model.Nombres)</th>
                                        <th>@Html.DisplayNameFor(model => model.Apellidos)</th>
                                        <th>Acciones</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                {
                                        <tr>
                                            <td>@Html.DisplayFor(modelItem => item.UserId)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Email)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Name)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Nombres)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Apellidos)</td>
                                            <td>@Html.ActionLink("Roles", "Roles", new { userId = item.UserId }, new { @class = "btn btn-success btn-sm" })</td>
                                        </tr>
                                }
                                </tbody>
                            </table>
                        </div>
                     </div>
                </div>
            </div>
        </div>    
<!-- /page content -->
@section scripts{
    @Scripts.Render("~/bundles/customJS")
}









